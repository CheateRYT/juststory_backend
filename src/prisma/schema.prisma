generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id           Int       @id @default(autoincrement())
  token        String
  name         String    @unique
  password     String    @unique
  subscription Boolean   @default(false)
  subBuyTime   DateTime?
  subEndTime   DateTime?
}
